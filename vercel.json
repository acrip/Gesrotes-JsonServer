{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/server.js"
      }
    ],
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }